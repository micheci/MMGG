<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>MMGG</title>
    <link href="https://unpkg.com/tailwindcss@^1.0/dist/tailwind.min.css" rel="stylesheet">
    <script src="js/fetch.js"></script>

</head>
<body>
    <div class=" flex  justify-center">
    <h1>hi</h1>
    <label for="summonerName">Summoner Name:</label>
    <input type="text" id="summonerName"/>

    <label for="searchSummoner"></label>
    <button onclick="searchSummoner()">Search Summoner</button>

    <div id="summoner_name"></div>
    <div id="summoner_level"></div>
    <div id="error"></div>

    
    <img id="summoner_pic" src="" alt="summoner pic">
    <div id="regionPicked"></div>

    <select name="regions" id="regions">
        <option value="0">Brazil</option>
        <option value="1">Europe North & East</option>
        <option value="2">Europe West</option>
        <option value="3">Japan</option>
        <option value="4">Korea</option>
        <option value="5">LAN</option>
        <option value="6">LAS</option>
        <option value="7">North America</option>       
        <option value="8">Oceania</option>
        <option value="9">Turkey</option>
        <option value="10">Russia</option>
        <option value="11">Phillippines</option>
        <option value="12">Singapore</option>
        <option value="13">Thailand</option>
    </select>
</div>

    <!-- Wins loss -->
    <h3>Wins:<div id="wins"></div></h3>
    <h3>Loses:<div id="losses"></div></h3>

    <!-- Tier/rank/lp -->
    <h3>Tier:<div id="tier"></div></h3>
    <h3>Rank:<div id="rank"></div></h3>
    <h3>LP:<div id="LP"></div></h3>
    <div id="winrate"></div>


   
  




</body>
</html>